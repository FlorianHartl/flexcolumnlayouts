    <file original="" product-name="TechDivision.NodeTypes.FlexColumnLayouts" source-language="en" datatype="plaintext">
        <body>
            <trans-unit id="properties.flexShrink" xml:space="preserve">
                <source>shrink content to fit</source>
            </trans-unit>
            <trans-unit id="properties.flexShrink.ui.help.message" xml:space="preserve">
                <source>"Allow shrinking" is the default behaviour, so the browser tries to place all columns in one line and may shrink cols if needed. Denied will affect that this column can not be shrinked by the browser, may be useful when using a custom "column width". Attention: this is an advanced option so use carefully! Further Documentation: https://yoksel.github.io/flex-cheatsheet/#section-flex-shrink</source>
            </trans-unit>
            <trans-unit id="properties.flexShrink.selectBoxEditor.values." xml:space="preserve">
                <source>none</source>
            </trans-unit>
            <trans-unit id="properties.flexShrink.selectBoxEditor.values.flex-shrink" xml:space="preserve">
                <source>explicitly allow shrinking</source>
            </trans-unit>
            <trans-unit id="properties.flexShrink.selectBoxEditor.values.flex-shrink-0" xml:space="preserve">
                <source>explicitly deny shrinking</source>
            </trans-unit>
        </body>
