                <source>none</source>
            </trans-unit>
            <trans-unit id="properties.flexDirection.selectBoxEditor.values.flex-row" xml:space="preserve">
                <source>within a row</source>
            </trans-unit>
            <trans-unit id="properties.flexDirection.selectBoxEditor.values.flex-row-reverse" xml:space="preserve">
                <source>backwards within a row</source>
            </trans-unit>
            <trans-unit id="properties.flexDirection.selectBoxEditor.values.flex-col" xml:space="preserve">
                <source>within a column</source>
            </trans-unit>
            <trans-unit id="properties.flexDirection.selectBoxEditor.values.flex-col-reverse" xml:space="preserve">
