                <target>gleichmäßiger Abstand innen</target>
            </trans-unit>
            <trans-unit id="properties.flexJustify.selectBoxEditor.values.justify-evenly" xml:space="preserve">
                <source>align evenly space around</source>
                <target>gleichmäßiger Abstand außen</target>
            </trans-unit>
            <trans-unit id="properties.flexJustify.selectBoxEditor.values.justify-start" xml:space="preserve">
